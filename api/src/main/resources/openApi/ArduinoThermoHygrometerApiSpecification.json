  "components": {
    "schemas": {
      "TemperatureDto": {
        "type": "object",
        "properties": {
          "registeredAt": {
            "type": "string",
            "format": "date-time"
          },
          "temp": {
            "type": "number",
            "format": "double",
            "maximum": 125,
            "minimum": -55
          }
        },
        "required": [
          "registeredAt"
        ]
      },
      "HumidityDto": {
        "type": "object",
        "properties": {
          "registeredAt": {
            "type": "string",
            "format": "date-time"
          },
          "airHumidity": {
            "type": "number",
            "format": "double",
            "maximum": 90,
            "minimum": 20
          }
        },
        "required": [
          "registeredAt"
        ]
      },
      "BatteryDto": {
        "type": "object",
        "properties": {
          "registeredAt": {
            "type": "string",
            "format": "date-time"
          },
          "batteryStatus": {
            "type": "integer",
            "format": "int32",
            "maximum": 100,
            "minimum": 0
          }
        },
        "required": [
          "registeredAt"
        ]
      }
    }
