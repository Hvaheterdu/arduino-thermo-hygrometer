{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "Database": {
    "RunDatabaseMigrationsOnStartup": true
  },
  "CORS": {
    "AllowedOrigin": "https://localhost:3001"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Stored in Azure for Staging, Test and Production environments. Set in appsettings.Development.json for local Development environment."
  },
  "HTTPS_PORTS": {
    "Development": 5001,
    "Production": 443
  }
}
